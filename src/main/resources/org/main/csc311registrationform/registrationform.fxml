<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="395.0" prefWidth="578.0" style="-fx-border-color: green;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.main.csc311registrationform.Contollers.RegistrationFormController">
   <children>
      <Label layoutX="183.0" layoutY="29.0" text="Registration Form">
         <font>
            <Font name="System Bold Italic" size="29.0" />
         </font>
      </Label>
      <VBox layoutX="-3.0" layoutY="92.0" prefHeight="366.0" prefWidth="587.0">
         <children>
            <HBox alignment="CENTER" prefHeight="78.0" prefWidth="587.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="78.0" prefWidth="577.0">
                     <children>
                        <Label text="First Name: " />
                        <TextField fx:id="firstNameTxtF" promptText="Enter First Name" />
                        <HBox alignment="CENTER" prefHeight="78.0" prefWidth="272.0">
                           <children>
                              <Label text="Last Name: " />
                              <TextField fx:id="lastNametxtF" promptText="Enter Last Name" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
                  <HBox />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="57.0" prefWidth="587.0">
               <children>
                  <Label text="Email:" />
                  <TextField fx:id="emailTxtF" promptText="Enter Email Address" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="59.0" prefWidth="587.0">
               <children>
                  <Label text="Date of Birth: " />
                  <TextField fx:id="dobTxtF" promptText="MM/DD/YYYY" />
                  <HBox alignment="CENTER" prefHeight="59.0" prefWidth="254.0">
                     <children>
                        <Label text="Zip Code" />
                        <TextField fx:id="zipcodeTxtF" promptText="Enter Zip Code" />
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="51.0" prefWidth="587.0">
               <children>
                  <Button fx:id="addBtn" disable="true" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="26.0" prefWidth="81.0" text="Add" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
